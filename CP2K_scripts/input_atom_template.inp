&GLOBAL
  PROJECT ##JOBNAME##
  RUN_TYPE ENERGY
  &PRINT MEDIUM
  &END PRINT
&END GLOBAL

&FORCE_EVAL
  METHOD QS
  STRESS_TENSOR ANALYTICAL
  &DFT
	CHARGE ##CHARGE##
	MULTIPLICITY 1
    BASIS_SET_FILE_NAME BASIS_pob
    POTENTIAL_FILE_NAME ##ALL_POTENTIAL_FILE##
    &MGRID
		CUTOFF ##CUTOFF##
		REL_CUTOFF 60
    &END MGRID
    &QS
		EPS_DEFAULT ##EPSDEFAULT##
		EPS_PGF_ORB ##EPSPGFORB##
		METHOD  GAPW
		EXTRAPOLATION USE_GUESS
    &END QS
    &SCF
		SCF_GUESS ATOMIC
		MAX_SCF    200
		EPS_SCF    ##EPSSCF##
##BRODEN##
    &END SCF
##XCFUNC##
##RELATIVISTIC##
    &POISSON
		PERIODIC NONE
		PSOLVER  WAVELET
    &END POISSON
  &END DFT
  &SUBSYS
    &CELL
      ABC ##BOXL## ##BOXL## ##BOXL##
	  ALPHA_BETA_GAMMA 90 90 90
	  PERIODIC NONE
    &END CELL
    &COORD
		SCALED
		##ATOM##    0.5 0.5 0.5
    &END COORD
    &TOPOLOGY
      &CENTER_COORDINATES
      &END CENTER_COORDINATES
    &END TOPOLOGY
##BASISSET##
  &END SUBSYS
&END FORCE_EVAL